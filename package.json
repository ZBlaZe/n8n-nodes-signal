{
  "name": "n8n-nodes-signal",
  "version": "0.2.0",
  "description": "Signal Node for n8n",
  "keywords": [
    "n8n-community",
    "signal",
    "messaging"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "tsc && copy nodes\\Signal\\signal.svg dist\\nodes\\Signal\\signal.svg",
    "dev": "tsc --watch"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "./dist/nodes/Signal/Signal.node.js",
      "./dist/nodes/Signal/SignalTrigger.node.js"
    ],
    "credentials": [
      "./dist/credentials/SignalApi.credentials.js"
    ]
  },
  "dependencies": {
    "@types/node": "20.12.12",
    "axios": "1.7.2",
    "n8n-core": "1.110.0",
    "n8n-workflow": "1.108.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.12",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  }
}
